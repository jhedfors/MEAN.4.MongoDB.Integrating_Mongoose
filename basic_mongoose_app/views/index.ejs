<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <div class="wrapper">
      <form class="" action="/users" method="post">
        Name:<input type="text" name="name" value="">
        Age:<input type="number" name="age" value="">
        <input type="submit" value="Submit">
      </form>


    <% for(var i in users) {%>
      <% var user = users[i] %>

      <p>
        Name: <%= user.name %>
      </p>
      <p>
        Age: <%= user.age %>
      </p>
    <% }%>

    </div>
  </body>
</html>
